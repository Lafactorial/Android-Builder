[
  {
    "kernelSource": {
      "name": "Sleepy-Kernel",
      "repo": "https://github.com/lafactorial/kernel_xiaomi_sm6150-sleepy",
      "branch": "sleepy-k6a",
      "device": "sweet_k6a",
      "defconfig": "vendor/sweetk6a.config",
      "defconfig": "vendor/sdmsteppe-perf_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://gitlab.com/itsshashanksp/android_prebuilts_clang_host_linux-x86_clang-r510928",
        "branch": "14.0",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/itsshashanksp/AnyKernel3/",
        "branch": "sweet_k6a"
      }
    },
    "enableKernelSU": false,
    "enableLXC": true
  }
]
