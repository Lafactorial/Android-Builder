[
  {
    "kernelSource": {
      "name": "Kernel",
      "repo": "https://github.com/Pulkit077/kernel_xiaomi_sweet_k6a.git",
      "branch": "lineage-21",
      "device": "sweet_k6a",
      "defconfig": "vendor/sweet_user_defconfig",
    },
    "toolchains": [
      {
        "repo": " https://gitlab.com/PixelOS-Devices/playgroundtc.git",
        "branch": "17",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/Lafactorial/AnyKernel31/",
        "branch": "sweetk6a"
      }
    },
    "enableKernelSU": false,
    "enableLXC": true
  }
]
