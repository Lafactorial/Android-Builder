[
  {
    "kernelSource": {
      "name": "Kewl-Kernel",
      "repo": "https://github.com/Pulkit077/kernel_xiaomi_sweet_k6a.git",
      "branch": "lineage-21",
      "device": "sweet_k6a",
      "defconfig": "vendor/sweet_user_defconfig"
    },
    "toolchains": [
      {
        "repo": " https://github.com/Lafactorial/android_prebuilts_clang_host_linux-x86_clang-r510928.git",
        "branch": "14.0",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
                              LLVM=1 ,
                              LLVM_IAS=1 ,
                              AR=llvm-ar ,
                              NM=llvm-nm ,
                              LD=ld.lld ,
                              OBJCOPY=llvm-objcopy ,
                              OBJDUMP=llvm-objdump ,
                              STRIP=llvm-strip
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/Lafactorial/AnyKernel31/",
        "branch": "sweetk6a"
      }
    },
    "enableKernelSU": false,
    "enableLXC": true
  }
]
